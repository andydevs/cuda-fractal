<?xml version="1.0" encoding="utf-8"?>
<fractals>
  <fractal mnemonic="flower-fractal">
    <constant real="-0.4" imag="0.6" />
    <scale rotate="0.0" zoom="0.75" />
    <translate transx="0.0" transy="0.0" />
    <image width="1920" height="1080" 
           filename="flower.png" />
    <colormap type="gradient">
      <from type="hex" hex="0x000000"/>
      <to type="hex" hex="0x00aaff"/>
    </colormap>
  </fractal>

  <fractal mnemonic="colorful-fractal">
    <constant real="-0.4" imag="0.6" />
    <scale rotate="0.0" zoom="0.75" />
    <translate transx="0.0" transy="0.0" />
    <image width="1920" height="1080"
           filename="colorful.png" />
    <colormap preset="orchid"/>
  </fractal>

  <fractal mnemonic="complex-fractal">
    <constant real="-0.4" imag="0.6" />
    <scale rotate="0.0" zoom="0.75" />
    <translate transx="0.0" transy="0.0" />
    <image width="1920" height="1080"
           filename="complex.png" />
    <colormap preset="noir"/>
  </fractal>

  <fractal mnemonic="mandelbrot-fractal" mandelbrot="true">
    <constant real="-0.4" imag="0.6" />
    <scale rotate="0.0" zoom="0.75" />
    <translate transx="0.0" transy="0.0" />
    <image width="1920" height="1080"
           filename="mandelbrot.png" />
    <colormap preset="nvidia"/>
  </fractal>
</fractals>